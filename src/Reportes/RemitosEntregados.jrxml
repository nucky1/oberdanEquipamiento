	</parameter>
	<queryString>
		<![CDATA[SELECT
     count(remito.`id`) AS cantidad_remito,
     remito.`id` AS remito_id,
     remito.`fecha_entrega` AS remito_fecha_entrega,
     remito.`entregado` AS remito_entregado,
     remito.`anulado` AS remito_anulado,
     remito.`fletero` AS remito_fletero,
     credito.`id` AS credito_id,
     cliente.`nombre` AS cliente_nombre,
     direccion.`id` AS direccion_id,
     direccion.`nombre` AS direccion_nombre,
     cliente.`id` AS cliente_id,
     cliente.`numero` AS cliente_numero
FROM
     `credito` credito INNER JOIN `remito` remito ON credito.`id` = remito.`credito_id`
     INNER JOIN `cliente` cliente ON credito.`cliente_id` = cliente.`id`
     INNER JOIN `direccion` direccion ON cliente.`direccion_id` = direccion.`id`
WHERE
     remito.`id` IN $P!{idRenglones}]]>
	</queryString>
